<%- include('./partials/head.ejs') %>
<div class="container">
  <h1 class="my-3">Production Houses</h1>
  <table class="table table-sm table-hover table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Headquarters</th>
        </tr>
    </thead>
    <tbody>
      <% productionHouses.forEach(productionHouse => { %>
        <tr>
          <td><%= productionHouse.name_prodHouse %></td>
          <td><%= productionHouse.headquarters %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>
<%- include('./partials/bottom.ejs') %>